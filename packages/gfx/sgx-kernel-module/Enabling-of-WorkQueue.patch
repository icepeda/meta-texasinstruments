From 708ae1255a7d96460d155997741efbcfd9646a13 Mon Sep 17 00:00:00 2001
From: Preshit Agarwal <preshit.agarwal@ti.com>
Date: Tue, 23 Feb 2010 13:49:31 +0530
Subject: [PATCH] Enabling of WorkQueue


Signed-off-by: Preshit Agarwal <preshit.agarwal@ti.com>
---
 .../linux/omap3630_linux/makefile.shared_conf      |    3 ++-
 1 files changed, 2 insertions(+), 1 deletions(-)

diff --git a/eurasiacon/build/linux/omap3630_linux/makefile.shared_conf b/eurasiacon/build/linux/omap3630_linux/makefile.shared_conf
index 5b2aebb..4f48c09 100755
--- a/eurasiacon/build/linux/omap3630_linux/makefile.shared_conf
+++ b/eurasiacon/build/linux/omap3630_linux/makefile.shared_conf
@@ -44,7 +44,8 @@ ARCH_CFLAGS		= -march=armv7-a
 # SYS_FLAGS contains any flags specific to this system
 SYS_CFLAGS		= -DSGX_DYNAMIC_TIMING_INFO \
 				-DSYS_CUSTOM_POWERLOCK_WRAP \
-				-DSYS_OCP_BYPASS
+				-DSYS_OCP_BYPASS \
+				-DPVR_LINUX_MISR_USING_WORKQUEUE
 
 # The version of the kernel that is required for compilation
 REQUIREDKERNELVERSION = 2.6.32
